<h1>Show Song</h1>

<ul class="list-group">
  <li class="list-group-item">
    <strong>Name:</strong>
    <%= @song.name %>
	</li>
  <audio
    controls
    src=<%= Routes.audio_path(@conn, :song, %{id: @song.id}) %>
  </audio>

</ul>

<span><%= link "Back", to: Routes.song_path(@conn, :index) %></span>
